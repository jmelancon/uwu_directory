<!doctype html>
<html lang="en-US" xml:lang="en-US">
<head>
    <meta name="viewport" content="width=device-width" />
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <style>
        body{
            font-family: "Roboto", "Helvetica", "Arial", sans-serif
        }

        h1{
            font-weight: 400;
        }

        a{
            color: #c950a7;
        }

        blockquote{
            border: 1px solid #ccc;
            background: #eee;
            margin-left: 8px;
            margin-right: 8px;
        }

        blockquote > p {
            margin: 4px;
            font-style: oblique;
        }
    </style>
    {% if config("emailStyles") is not null and config("emailStyles") is not empty %}
        <style>
            {{ config("emailStyles")|raw }}
        </style>
    {% endif %}
    <title></title>
</head>
<body class="">
<h1>Hello{% if name is defined %} {{ name }}{% endif %},</h1>
{% block body %}
{% endblock %}
<p>Many thanks,</p>

{% if config("emailSignature") is not null and config("emailSignature") is not empty %}
    {{ config("emailSignature")|raw }}
{% else %}
    <!--suppress HtmlDeprecatedAttribute -->
    <table role="presentation" border="0" cellpadding="0" cellspacing="0" class="body" style="border: 1px solid #ccc; background: #eee;">
        <tr>
            <td style="background: linear-gradient(to right, #c950a7, #eee); padding: 8px;">
                <!--suppress CheckImageSize -->
                <img
                    src="data:image/png;base64,"
                    alt="uwu_directory logo. It's a hibiscus flower emoji."
                    width="128"
                    height="128"
                    style="width: 128px; height: 128px;"
                />
            </td>
            <td style="background: #eee; padding: 8px;">
                <b>{{ config("organization")|default("uwu_directory") }}</b>
                <p><a href="https://github.com/jmelancon/uwu_directory">jmelancon/uwu_directory</a><p>
            </td>
        </tr>
    </table>
{% endif %}
</body>
</html>